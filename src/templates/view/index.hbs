                </div>
            </div>
            <div id="category-filter">
                {{#each book.item}}
                <ul class="category-list">
                    <li class="category-item {{this.categories}}">
                        <figure>
                            <img src="images/book-img/{{this.buk}}" alt="img" />
                            <figcaption class="bg-orange">
                                <div class="info-block">
                                    <h4>{{this.title}}</h4>
                                    <span class="author"><strong>Author:</strong> {{this.author}}</span>
